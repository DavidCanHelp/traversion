{
  "dashboard": {
    "id": null,
    "title": "Traversion Causality Intelligence",
    "tags": ["traversion", "causality", "debugging"],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Causality Detection Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(traversion_causality_detections_total[2m])",
            "interval": "",
            "legendFormat": "{{tenant_id}} - {{causality_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Detections/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 2,
        "title": "Causality Graph Size",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "traversion_causality_graph_size",
            "interval": "",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10000},
                {"color": "red", "value": 100000}
              ]
            }
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        }
      },
      {
        "id": 3,
        "title": "Confidence Distribution",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "traversion_causality_confidence",
            "interval": "",
            "legendFormat": "{{tenant_id}} {{causality_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "buckets": {
            "start": 0,
            "end": 1,
            "count": 20
          }
        }
      },
      {
        "id": 4,
        "title": "Causality Types Breakdown",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum by (causality_type) (rate(traversion_causality_detections_total[5m]))",
            "interval": "",
            "legendFormat": "{{causality_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["percent", "value"]
          }
        }
      },
      {
        "id": 5,
        "title": "Pattern Detection",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "rate(traversion_patterns_detected_total[5m])",
            "interval": "",
            "legendFormat": "{{pattern_type}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Patterns/sec",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 6,
        "title": "Anomaly Score Distribution",
        "type": "histogram",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "traversion_event_anomaly_score",
            "interval": "",
            "legendFormat": "{{tenant_id}}",
            "refId": "A"
          }
        ],
        "options": {
          "buckets": {
            "start": 0,
            "end": 1,
            "count": 10
          }
        }
      },
      {
        "id": 7,
        "title": "Causality Chain Length",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(traversion_causality_chain_length_bucket[5m]))",
            "interval": "",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(traversion_causality_chain_length_bucket[5m]))",
            "interval": "",
            "legendFormat": "P50",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Chain Length",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 8,
        "title": "Root Cause Analysis Speed",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(traversion_root_cause_analysis_duration_seconds_bucket[5m]))",
            "interval": "",
            "legendFormat": "P95 Analysis Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(traversion_root_cause_analysis_duration_seconds_bucket[5m]))",
            "interval": "",
            "legendFormat": "P50 Analysis Time",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 9,
        "title": "Top Services by Causality Events",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "topk(10, sum by (service_id) (rate(traversion_causality_detections_total[5m])))",
            "interval": "",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "service_id": "Service",
                "Value": "Detections/sec"
              }
            }
          }
        ]
      },
      {
        "id": 10,
        "title": "Causality Confidence Over Time",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "increase(traversion_causality_confidence_bucket[2m])",
            "interval": "",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "heatmap": {
          "xBucketSize": "2m",
          "yBucketSize": 0.05,
          "yBucketBound": "upper"
        },
        "dataFormat": "tsbuckets"
      },
      {
        "id": 11,
        "title": "Event Processing Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(traversion_event_processing_duration_seconds_bucket[5m]))",
            "interval": "",
            "legendFormat": "P95 Processing Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(traversion_event_processing_duration_seconds_bucket[5m]))",
            "interval": "",
            "legendFormat": "P50 Processing Time",
            "refId": "B"
          },
          {
            "expr": "rate(traversion_events_total[5m])",
            "interval": "",
            "legendFormat": "Events/sec (right axis)",
            "refId": "C",
            "yAxis": 2
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0,
            "position": "left"
          },
          {
            "label": "Events/sec",
            "min": 0,
            "position": "right"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(traversion_causality_detections_total, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "causality_type",
          "type": "query",
          "query": "label_values(traversion_causality_detections_total{tenant_id=~\"$tenant\"}, causality_type)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Pattern Alerts",
          "datasource": "prometheus",
          "expr": "ALERTS{alertname=\"traversion_causality_detection_anomaly\"}",
          "titleFormat": "Causality Anomaly",
          "textFormat": "Unusual causality detection patterns detected"
        }
      ]
    }
  }
}